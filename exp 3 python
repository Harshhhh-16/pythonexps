task = []
task.append("clean")
task.append("hw")
task.append("project")
print(task)
task.insert(0, "vegetable")
print(task)
task.remove("hw")
print(task)
task.sort()
print(task)
task.reverse()
print(task)
task.clear()
print(task)
todo_list = ()
todo_list += ("clean",)
todo_list += ("hw",)
todo_list += ("project",)
print(todo_list)
todo_list = tuple(task for task in todo_list if task != "hw")
print(todo_list)
todo_list = ("vegetable",) + todo_list
todo_list = tuple(sorted(todo_list))
print(todo_list)
todo_list = tuple(reversed(todo_list))
print(todo_list)
todo_list = ()
print(todo_list)
